            "args": ["--video", "data/remote/remote_video.MOV", "--feature_frames", "5", "--frame_skip", "15", "--image", "data/remote/remove_img_1.jpg"],
            "justMyCode": true
        },
        {
            "name": "Python: Launch Feature Builder",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/build_feature_set.py",
            "args": [
                "--input",
                "data/stuffedanimal/stuffedanimal_video.MOV",
                "--feature_file",
                "data/remote_video/output.json",
                "--frame_skip",
                "15"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
